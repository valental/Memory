<Application x:Class="MemoryWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MemoryWPF"
             xmlns:converters="clr-namespace:MemoryWPF.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVis"/>
        <converters:InvertBoolToVisibilityConverter x:Key="InvertBoolToVisConverter"/>
        <converters:ThemeToStringConverter x:Key="ThemeToStringConverter"/>
        <converters:ThemeToColorConverter x:Key="ThemeToColorConverter"/>
        <converters:ThemeAndIdToNameConverter x:Key="ThemeAndIdToNameConverter"/>
        <converters:ThemeAndIdToImageConverter x:Key="ThemeAndIdToImageConverter"/>
    </Application.Resources>
</Application>
